<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>GetCreateUserServlet</servlet-name>
        <servlet-class>controller.GetCreateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostCreateUserServlet</servlet-name>
        <servlet-class>controller.PostCreateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetUpdateUserServlet</servlet-name>
        <servlet-class>controller.GetUpdateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostUpdateUserServlet</servlet-name>
        <servlet-class>controller.PostUpdateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserServlet</servlet-name>
        <servlet-class>controller.DeleteUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListUsersServlet</servlet-name>
        <servlet-class>controller.ListUsersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetCreateUserServlet</servlet-name>
        <url-pattern>/GetCreateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostCreateUserServlet</servlet-name>
        <url-pattern>/PostCreateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetUpdateUserServlet</servlet-name>
        <url-pattern>/GetUpdateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostUpdateUserServlet</servlet-name>
        <url-pattern>/PostUpdateUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserServlet</servlet-name>
        <url-pattern>/DeleteUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListUsersServlet</servlet-name>
        <url-pattern>/ListUsersServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
